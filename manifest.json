{
  "name": "Brandon's GMAIL Extension",
  "description": "Extension for chrome to ease some more menial tasks.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["content.min.js"],
      "css": ["content.min.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2
}
